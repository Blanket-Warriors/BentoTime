{
  "name": "BentoTime",
  "version": "1.0.0",
  "description": "An application for reading Manga",
  "repository": "https://github.com/Blanket-Warriors/BentoTime",
  "main": "public/main.js",
  "scripts": {
    "start": "npm run electron:dev & npm run watch",
    "test": "export NODE_ENV='test'; cd test && karma start",
    "packager": "npm run build:prod && electron-packager ./public BentoTime --all --out=./public/packages --overwrite",
    "electron:prod": "export NODE_ENV='production';  npm run build && electron ./public/",
    "electron:dev": "export NODE_ENV='development'; npm run build && electron ./public/",
    "watch": "export NODE_ENV='development'; npm run watch:main & npm run server:renderer",
    "build": "npm run clean && npm run build:main && npm run build:renderer",
    "build:prod": "export NODE_ENV='production';  npm run build;",
    "build:dev": "export NODE_ENV='development'; npm run build;",
    "server:renderer": "webpack-dev-server --config webpack/webpack.renderer.js --progress --colors --inline",
    "build:main": "webpack --config webpack/webpack.main.js   --progress --colors --inline",
    "build:renderer": "webpack --config webpack/webpack.renderer.js --progress --colors --inline",
    "watch:main": "webpack --config webpack/webpack.main.js   --progress --colors --inline --watch",
    "watch:renderer": "webpack --config webpack/webpack.renderer.js --progress --colors --inline --watch",
    "clean": "rm -rf public/build && rm -rf public/packages"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "author": "Blanket Warriors",
  "license": "ISC",
  "engine": "node >= 4.0.0",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "electron-debug": "^1.0.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.2.5",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.5.4",
    "inject-loader": "^2.0.1",
    "json-loader": "^0.5.4",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.1.0",
    "sass-loader": "^3.2.1",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
