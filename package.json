{
  "name": "BentoTime",
  "version": "0.1.2",
  "description": "An application for reading Manga",
  "repository": "https://github.com/Blanket-Warriors/BentoTime",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "start": "npm run dev-electron & npm run dev-server",
    "build": "export NODE_ENV='production'; npm run clean && webpack --config webpack/webpack.production.js --progress --colors --inline",
    "packager": "electron-packager ./public BentoTime --all --out=./public/packages --overwrite",
    "prod-electron": "export NODE_ENV='production'; electron ./public/",
    "dev-build": "export NODE_ENV='development'; npm run clean && webpack --config webpack/webpack.development.js --progress --colors --inline",
    "dev-electron": "export NODE_ENV='development'; electron ./public/",
    "dev-server": "export NODE_ENV='development'; webpack-dev-server --config webpack/webpack.development.js --progress --colors --inline",
    "clean": "rm -rf public/build"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ]
  },
  "author": "Blanket Warriors",
  "license": "ISC",
  "engine": "node >= 4.0.0",
  "dependencies": {
    "lodash": "^4.0.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-logger": "^2.3.2",
    "redux-thunk": "^1.0.3",
    "rx": "^4.0.0",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "electron-builder": "^3.1.1",
    "electron-debug": "^0.5.1",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.36.4",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.11.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "inject-loader": "^2.0.1",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.1",
    "source-map-support": "^0.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
