{
  "name": "BentoTime",
  "version": "0.2.0",
  "description": "An application for reading Manga",
  "repository": "https://github.com/Blanket-Warriors/BentoTime",
  "main": "public/index.js",
  "scripts": {
    "test": "cd test && karma start",
    "start": "npm run dev-electron & npm run dev-server",
    "build": "export NODE_ENV='production'; npm run clean && webpack --config webpack/webpack.production.js --progress --colors --inline",
    "packager": "electron-packager ./public BentoTime --all --out=./public/packages --overwrite",
    "prod-electron": "export NODE_ENV='production'; electron ./public/",
    "dev-build": "export NODE_ENV='development'; npm run clean && webpack --config webpack/webpack.development.js --progress --colors --inline",
    "dev-electron": "export NODE_ENV='development'; electron ./public/",
    "dev-server": "export NODE_ENV='development'; webpack-dev-server --config webpack/webpack.development.js --progress --colors --inline",
    "clean": "rm -rf public/build"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ]
  },
  "author": "Blanket Warriors",
  "license": "ISC",
  "engine": "node >= 4.0.0",
  "dependencies": {
    "lodash": "^4.7.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "rx": "^4.1.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "electron-debug": "^0.6.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.37.3",
    "eslint": "^2.5.3",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "inject-loader": "^2.0.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
